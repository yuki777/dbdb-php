{
    "name": "yuki777/dbdb-php",
    "type": "composer-plugin",
    "require": {
        "composer-plugin-api": "^2.0"
    },
    "require-dev": {
        "ext-pdo_mysql": "*",
        "composer/composer": "^2.0",
        "phpunit/phpunit": "^10.0",
        "squizlabs/php_codesniffer": "^3.7"
    },
    "autoload":{
        "psr-4":{
            "DbdbPhp\\Composer\\":"src/"
        }
    },
    "extra":{
        "class":"DbdbPhp\\Composer\\Plugin"
    },
    "license": "MIT",
    "config": {
        "sort-packages": true
    }
}
