{
    "name": "yuki777/dbdb-php",
    "description": "Database version manager for MySQL, PostgreSQL, MongoDB & Redis",
    "keywords": [
        "database",
        "version",
        "manager",
        "MySQL",
        "PostgreSQL",
        "MongoDB",
        "Redis",
        "plugin",
        "composer"
    ],
    "type": "composer-plugin",
    "require": {
        "composer-plugin-api": "^2.0"
    },
    "require-dev": {
        "ext-pdo_mysql": "*",
        "composer/composer": "^2.6",
        "phpunit/phpunit": "^10.4",
        "squizlabs/php_codesniffer": "^3.7"
    },
    "autoload":{
        "psr-4":{
            "DbDbPhp\\Composer\\":"src/"
        }
    },
    "extra":{
        "class":"DbDbPhp\\Composer\\Plugin"
    },
    "license": "MIT",
    "config": {
        "sort-packages": true
    }
}
